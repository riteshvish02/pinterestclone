  <link rel="stylesheet" href="/stylesheets/feed.css">
  <style>
    /*ROOT*/
*{
  outline:none;

}
body{
  margin:0px;
  padding:0px;
  font-family: arial;
}
#main{
  position: relative;
}
img{
  user-select:none;
}

/*MENU*/
.menu{
  max-width:100%;
  display:flex;
  background-color: #fff;
  height: 50px;
  align-items:center;
  position: sticky;
  top: 0;
  padding: 0 15px;
  z-index: 99999;
  font-weight:bold;
}
.menu .logo{
  width:15px;
  padding:10px;
  cursor:pointer;
  border-radius:50%;
}
.menu .logo:hover{
  background:#eeeeee;
}
.menu .home{
  color:#000;
  
  cursor:pointer;
  user-select:none;
  font-size: 10px;
  font-weight: 550;
  border-radius:40px;
  margin:0px 5px;
}
.menu .search-bar{
  width:100%;
  margin:0px 5px;
  border-radius:40px;
  outline:none;
  padding: 4px;
  position:relative;
  background-color:#efefef;
  font-size:10px;
  /* background-image:url("https://cdn4.iconfinder.com/data/icons/music-ui-solid-24px/24/search-3-256.png"); */
  background-size:21px;
  background-repeat:no-repeat;
  background-position:center left;
  background-position:10px;
  color:gray;
  border:4px solid transparent;
}
.menu .search-bar:hover{
  background-color:#3333;
}
.menu .search-bar:focus{
  border:0.1px solid #7bb5f0;
  padding: 5px;
  background-image:none;
}
.menu .search-bar::placeholder{
  user-select:none;
}
.icon-container{
  display:flex;
  justify-content:center;
  align-items:center;
  position:relative;
}
.icon-container .icon{
  width: 15px;
  border-radius:50%;
  cursor:pointer;
  padding:8px;
}
.icon-container .icon:hover{
  background-color:#eeeeee;
}
svg{
  padding:4px 8px!important;
}
.icon-container .btn{
  width:10px;
  padding:5px;
}
.focus-bar{
  width:250px;
  height:350px;  border-radius:15px;
  position: absolute;
  background:#fff;
  border:2px solid #3333;
  position:absolute;
  top:40px;/*50px*/
  left:-150px;
  visibility:hidden;
  opacity:0;
  transition:all 0.3s;
  display:flex;
  justify-content:center;
  align-items:center;
  align-content:center;
  user-select:none;
  z-index:99;
}
.bell:focus + .message + .pp + .btn + .bell-bar, .bell-bar:focus{
  visibility:visible;
  opacity:1;
  top:50px;
}
.message:focus + .pp + .btn + .bell-bar + .message-bar, .message-bar:focus{
  visibility:visible;
  opacity:1;
  top:50px;
}
.pp:focus + .btn + .bell-bar + .message-bar + .pp-bar, .pp-bar:focus{
  visibility:visible;
  opacity:1;
  top:50px;
}
.btn:focus + .bell-bar + .message-bar + .pp-bar + .btn-bar, .btn-bar:focus{
  visibility:visible;
  opacity:1;
  top:50px;
}
.pp-bar a{
  text-decoration:none;
  padding:5px 15px;
  border:2px solid #161b22;
  color:#161b22;
  border-radius:5px;
}
.pp-bar{
  flex-direction:column;
}
.pp-bar img{
  width:150px;
  border-radius:50%;
  margin-bottom:20px;
}
.pp-bar a:hover{
  background:#161b22;
  color:white;
}

/*MINI SEARCH BAR*/
#two{
  display:none;
  padding:15px;
}
#anchor:hover{
 background-color: transparent!important;
}

  </style>
</head>
